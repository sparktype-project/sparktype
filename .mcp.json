
 {
 "mcpServers": {
    "browser-tools": {
      "command": "wsl",
      "args": [
        "npx @agentdeskai/browser-tools-mcp@1.2.0"
      ],
      "enabled": true
    }
  }
},{
  "mcpServers": {
    "tauri-mcp": {
      "command": "node",
      "args": ["/path/to/tauri-mcp/server/index.js"],
      "env": {
        "TAURI_MCP_LOG_LEVEL": "info"
      }
    }
  }
}