{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "http:default",
    "http:allow-fetch",
    {
      "identifier": "http:allow-fetch",
      "allow": [
        {
          "url": "https://**"
        },
        {
          "url": "http://**"
        }
      ]
    },
    "fs:default",
    "fs:allow-write-file",
    "fs:allow-create",
    "fs:allow-exists",
    "fs:allow-remove",
    "fs:allow-copy-file",
    "fs:allow-mkdir",
    "fs:allow-read-file",
    "dialog:default",
    "dialog:allow-save",
    "core:path:default",
    "default"
  ]
}
